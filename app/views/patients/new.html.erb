<%= link_to("<< Back to List", patients_path, :class => 'back-link') %>

<div class="patients new">
  <h2>Add Patient</h2>

  <%= form_for(@patient, :url => patients_path, :method => 'post') do |f| %>

    <%= render(:partial =>'form', :locals => {:f => f}) %>

    <div class="form-buttons">
      <%= f.submit("Add Patient") %>
    </div>

  <% end %>
</div>